<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="AdwPreferencesDialog" id="_preference_window">
    <child>
      <object class="AdwPreferencesPage">
        <child>
          <object class="AdwPreferencesGroup">
            <child>
              <object class="AdwComboRow" id="_quality_row">
                <property name="model">
                  <object class="GtkStringList">
                    <property name="strings">Low 96k
High 320k
Lossless
Hi-res Lossless</property>
                  </object>
                </property>
                <property name="title" translatable="yes">Quality</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup">
            <child>
              <object class="AdwComboRow" id="_sink_row">
                <property name="model">
                  <object class="GtkStringList">
                    <property name="strings">Automatic
PulseAudio
ALSA
Jack
OSS</property>
                  </object>
                </property>
                <property name="title" translatable="yes">Preferred Audio Sink</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup">
            <child>
              <object class="AdwSwitchRow" id="_normalize_row">
                <property name="title" translatable="yes">Normalize volume</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup">
            <child>
              <object class="AdwSwitchRow" id="_background_row">
                <property name="title" translatable="yes">Run in the background</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwPreferencesGroup">
            <child>
              <object class="AdwSwitchRow" id="_quadratic_volume_row">
                <property name="title" translatable="yes">Use quadratic volume control</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
